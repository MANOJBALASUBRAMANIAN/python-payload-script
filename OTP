for i in range(1,9999999):
    print(i)

for i in range(1,9):
print('{num:02d}'.format(num=i))

for i in range(10):
'{0:08}'.format(i)

for i in (1, 10, 100):
print('{num:02d}'.format(num=i))

for i in range(100000000):
print('{:d}'.format(i).zfill(8))



x = [1, 10, 100]
for i in x:
    print '%02d' % i
